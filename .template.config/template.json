{
  "$schema": "http://json.schemastore.org/template",
  "author": "Mikheil Berishvili",
  "classifications": [
    "Api",
    "Console",
    "Web"
  ],
  "identity": "Net.Template",
  "name": ".Net Template",
  "shortName": "nettemplate",
